  <command name="pushDataCommand">
    <task>
      <ref-class>com.travelsky.task.pushtask.SchedulerPushTask</ref-class>
      <description>push task</description>
    </task>
    <iterator>
      <ref-class>com.travelsky.pub.util.scheduling.iterators.PeriodsIterator</ref-class>
      <parameter>
        <startTime>
          <year>2014</year>
          <month>1</month>
          <day>6</day>
          <hour>13</hour>
          <minute>30</minute>
          <second>0</second>
        </startTime>
        <endTime>
          <year>3014</year>
          <month>10</month>
          <day>31</day>
          <hour>12</hour>
          <minute>0</minute>
          <second>0</second>
        </endTime>
        <period type="second">30</period>
      </parameter>
      <description>time period</description>
    </iterator>
  </command>
